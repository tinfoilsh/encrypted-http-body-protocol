    "@hpke/dhkem-x25519": "^1.6.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.25.9",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
